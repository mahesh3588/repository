<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="dao.ledger.quotation.QuotationItem" table="TB_Quotation_Item">  
    <id name="id" column="id">  
     <generator class="increment"></generator>  
    </id>  
	
    <property name="quotationId" column="quotation_id"></property>  
    <property name="quotationDate" column="quotation_date"></property>  
    <property name="itemId" column="item_id"></property>
    <property name="itemName" column="item_name"></property>
    <property name="quantity" column="quantity"></property>
    <property name="price" column="price"></property>
    <property name="vat" column="vat"></property>
    <property name="discount" column="discount"></property>
    
  </class>  
            
 </hibernate-mapping>  